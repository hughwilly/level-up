{
  "name": "hughwilly/level-up",
  "description": "Neat helpers for any Laravel project",
  "keywords": [
    "hughwilly",
    "level-up",
    "laravel",
    "lumen",
    "helpers"
  ],
  "homepage": "https://github.com/hughwilly/level-up",
  "license": "MIT",
  "authors": [
    {
      "name": "Henrik Nordquist",
      "email": "henrik@henriknordquist.dk",
      "homepage": "https://henriknordquist.dk",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : "^7.1",
    "illuminate/support": "~5.6"
  },
  "autoload": {
    "psr-4": {
      "HughWilly\\LevelUp\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "HughWilly\\LevelUp\\LevelUpServiceProvider"
      ],
      "aliases": {
        "ModelHelper": "HughWilly\\LevelUp\\Facades\\ModelHelper"
      }
    }
  },
  "require-dev": {
    "illuminate/database": "^5.6"
  }
}
